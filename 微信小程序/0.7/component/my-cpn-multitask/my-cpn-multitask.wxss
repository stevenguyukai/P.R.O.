
.head{
  font-size:60rpx;
  position: relative;
  left: 14vw;
} 

.aimIcon{
  width: 16vh;
  height: 16vh;
  position: absolute;
  top: 1vh;
  left: 1vh;
}

.topView{
  width: 100%;
  height: 20vh;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
 
.middleView{
  width: 70%;
  height: 65vh;
  background-color: white;
  z-index: 20;
  position:absolute;
  left:15%;
  top: 17.5vh;
  border-radius: 45rpx;
  opacity: 1;
}

.blurView{
  height:120%;
  width:100%;
  background-color: #7c7c7c;
  position:absolute;
  opacity: 0.66;
  top:-20%;
}



.separateLine{
  height: 2rpx;
  width: 60%;
  border-top: 3px dashed black;
  position: absolute;
  top: 36vh;
  z-index: 20;
  left: 20%;
}

.contant{
  height: 45.5vh;
  width: 60%;
  position: absolute;
  top: 37vh;
  z-index: 20;
  left: 20%;
}

.title{
  font-size: 35rpx;
  color: purple;
  display: block;
}

.describe{ 
  color: black;
  font-size: 28rpx;
  display: block;
}

.skipLine{
  height: 30rpx;
  display: block;
}
